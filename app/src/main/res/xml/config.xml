<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ceb.business" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>CebBusiness</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <!--<content src="http://192.168.0.103:8080/mpoin/#" />-->
    <!--<access origin="*" />-->
    <!--<allow-navigation href="http://*/*" />-->
    <!--<allow-navigation href="https://*/*" />-->
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="UtilPlugin">
        <param name="android-package" value="com.ceb.plugins.utils.UtilPlugin" />
    </feature>
    <feature name="CommonDevice">
        <param name="android-package" value="  com.ceb.plugins.devices.nt.CommonDevicePlugin" />
    </feature>
    <feature name="PinPad">
        <param name="android-package" value="com.ceb.plugins.devices.nt.PinPadPlugin" />
    </feature>
    <feature name="Swipe">
        <param name="android-package" value="com.ceb.plugins.devices.nt.SwipePlugin" />
    </feature>
    <feature name="Navigation">
        <param name="android-package" value="com.ceb.plugins.devices.nt.NavigationPlugin" />
    </feature>
</widget>
